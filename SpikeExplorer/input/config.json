{
    "hidden_layers": [100],
    "dataset":"mnist",
    "name": "fully_params_on_mnist",
    "overwrite_existing_experiment": true,
    "is_test": false,
    "num_trials": 20,
    "batch_size": 256,
    "num_epochs": 10,
    "num_steps": 25,
    "objectives": [
        {
            "obj_name": "accuracy",
            "minimize": false,
            "threshold": 0.98
        },
        {
            "obj_name": "time",
            "minimize": true,
            "threshold": 0.09
        }
    ],
    "parameters_to_search": [
        {
            "name": "lr",
            "type": "range",
            "bounds": [1e-4, 1e-2],
            "log_scale": true,
            "value_type": "float"
        },
        {
            "name": "beta1",
            "type": "range",
            "bounds": [0.95, 0.999],
            "log_scale": false,
            "value_type": "float"
        },
        {
            "name": "neuron_type",
            "type": "choice",
            "value_type": "str",
            "values": ["lif","rlif","syn","rsyn"]
        },
        {
            "name": "num_steps",
            "type": "choice",
            "value_type": "int",
            "values": [25,50,100]
        },
        {
            "name": "exp_decay",
            "type": "range",
            "bounds": [0.6, 0.95],
            "log_scale": false,
            "value_type": "float"
        }
    ],
    "neuron_consumption": {
        "active": 0.1,
        "idle": 0.001
    }
}