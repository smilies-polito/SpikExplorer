{
    "input_size": 784,
    "hidden_layers": [1000, 100],
    "output_size": 10,
    "dataset":"mnist",
    "name": "experiment_name",
    "overwrite_existing_experiment": true,
    "is_test": false,
    "num_trials": 20,
    "batch_size": 256,
    "num_epochs": 5,
    "num_steps": 25,
    "objectives": [
        {
            "obj_name": "accuracy",
            "minimize": false,
            "threshold": 0.98
        },
        {
            "obj_name": "time",
            "minimize": true,
            "threshold": 0.3
        }
    ],
    "parameters_to_search": [
        {
            "name": "lr",
            "type": "range",
            "bounds": [1e-4, 0.4],
            "log_scale": true,
            "value_type": "float"
        },
        {
            "name": "beta1",
            "type": "range",
            "bounds": [0.5, 0.999],
            "log_scale": false,
            "value_type": "float"
        }
    ],
    "neuron_consumption": {
        "active": 0.1,
        "idle": 0.001
    }
}